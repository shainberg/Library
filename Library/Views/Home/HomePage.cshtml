<script src="https://maps.googleapis.com/maps/api/js"></script>
<html>
<head>
    <script>
        $(function () {
            var elem = document.getElementById('theBestCanvasEver');
            var ctx = elem.getContext('2d');
            // Draw black rect
            ctx.fillRect(60, 42, 114, 114);

            // Draw blue rect
            ctx.fillStyle = "rgb(0, 162, 232)";
            ctx.fillRect(108, 78, 102, 102);

            // Increase line width
            ctx.lineWidth = 5;

            // Draw black rect outline
            ctx.strokeStyle = "rgb(0, 0, 0)";
            ctx.strokeRect(60, 228, 114, 114);

            // Draw blue rect outline
            ctx.strokeStyle = "rgb(0, 162, 232)";
            ctx.strokeRect(36, 64, 34, 34);

            // Draw transparent yellow rect
            ctx.fillStyle = "rgba(255, 255, 0, 0.75)";
            ctx.fillRect(52, 46, 34, 34);

            ctx.strokeStyle = "rgb(0, 0, 0)";
            ctx.font = '48px serif';
            ctx.strokeText("I have always imagined that paradise", 100, 250);
            ctx.strokeText("will be a kind of library", 250, 300);
            ctx.restore();
        });
    </script>
</head>
<body>
    <canvas id="theBestCanvasEver" width="1000" height="500">
        Your browser does not support canvas. Upgrade yourself.
    </canvas>
</body>
</html>